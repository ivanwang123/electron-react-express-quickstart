{
  "name": "electron-react-express-quickstart",
  "productName": "Electron+React+Express Quickstart",
  "main": "electron.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.2.0",
    "portfinder": "^1.0.28",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "electron-build": "robocopy ./ ./build electron.js & robocopy ./server ./build/server",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.0"
  },
  "build": {
    "appId": "com.ivanwang.quickstart",
    "win": {
      "target": "portable"
    },
    "portable": {
      "unicode": false,
      "artifactName": "Test.exe"
    }
  }
}
